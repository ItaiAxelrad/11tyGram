---
title: Followers
layout: base.njk
eleventyExcludeFromCollections: true
permalink: "followers/page-{{ pagination.pageNumber + 1 }}/index.html"
pagination:
  data: connections.followers
  size: 120
  alias: followers
---

<div id="{{ title | lower }}">
<ul class="{{ title | lower }}">
{%- for follower in followers %}
  <li class="follower">
    <a href="https://www.instagram.com/{{ follower }}/?hl=en" rel="noopener">@{{ follower }}</a>
  </li>
{% endfor -%}
</ul>
</div>

<nav class="pageNav">
  <div class="previous-page">
    {%- if pagination.href.previous %}
      <a href="{{ pagination.href.previous }}">
       &larr; Previous Page</a>
    {%- else %}
      <a href="/">&larr; Go Back</a>
    {%- endif %}
  </div>
  <div class="next-page">
    {%- if pagination.href.next %}
      <a href="{{ pagination.href.next }}">Next Page &rarr;
  </a>
      {%- else %}
      {%- endif %}
    </div>
  </nav>