{
  "name": "11tygram",
  "version": "1.0.0",
  "description": "A static site for your photo memories",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "watch": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "update": "npm i -g npm-check-updates && ncu -u && npm install && npm audit fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ItaiAxelrad/11tyGram.git"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "static",
    "site",
    "photo",
    "instagram",
    "clone"
  ],
  "author": {
    "name": "Itai Axelrad",
    "url": "https://itaiaxelrad.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "luxon": "^3.5.0"
  }
}
